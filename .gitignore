# Ignore bundler config.
/.bundle

# Vagrantfile
/.vagrant

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# ignore scraped things
/public/img
/public/items
/public/uploads/*
/public/images/img

# ignore rubymine config
/.idea

# ignore coverage reports
/spec/coverage

# Ignore application configuration
/config/application.yml

#all generated documentation
/doc

#secret
.secret
.env

# pipe down in there, redis
stdout
